<?xml version="1.0"?>
<package>
	<name>diagnostics</name>
	<version>0.0.0</version>
	<description>Provides a Qt gui for robot diagnostics</description>

	<maintainer email="max.schwarz@uni-bonn.de">Max Schwarz</maintainer>
	<license>TODO</license>

	<buildtool_depend>catkin</buildtool_depend>
	<build_depend>roscpp</build_depend>
	<build_depend>rqt_gui</build_depend>
	<build_depend>rqt_gui_cpp</build_depend>
	<build_depend>robotcontrol</build_depend>
	<run_depend>roscpp</run_depend>
	<run_depend>rqt_gui</run_depend>
	<run_depend>rqt_gui_cpp</run_depend>
	<run_depend>robotcontrol</run_depend>

	<export>
		<rqt_gui plugin="${prefix}/plugin_rqt_gui.xml" />
	</export>
</package>